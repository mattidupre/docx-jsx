{
  "type": "module",
  "name": "docx-jsx",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "files": [],
  "engines": {
    "node": ">=19",
    "pnpm": ">=8"
  },
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components.d.ts",
      "default": "./dist/components.js"
    }
  },
  "devDependencies": {
    "@types/react": "~18.2.25",
    "@types/react-dom": "~18.2.10",
    "@types/uuid": "^9.0.6",
    "esbuild": "^0.19.5",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.28.1",
    "prettier": "^3.0.3",
    "tsc-alias": "^1.8.8",
    "type-fest": "^4.3.3",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "react": "18",
    "react-dom": "18"
  },
  "scripts": {
    "backend:build": "rm -rf dist-backend/* ||: && tsc --declaration --project ./tsconfig-dist.json",
    "backend:dev": "pnpm run backend:build --watch",
    "headless:build": "rm -rf dist-headless/* ||: && esbuild src/headless.ts --bundle --format=iife --outdir=dist-headless",
    "headless:dev": "pnpm run headless:build --watch",
    "test": "vitest"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.31.18",
    "docx": "^8.2.3",
    "jszip": "^3.10.1",
    "pagedjs": "^0.4.3",
    "puppeteer": "^21.4.1",
    "uuid": "^9.0.1",
    "vite": "^4.5.0",
    "vitest": "^0.34.6",
    "xml-formatter": "^3.5.0"
  }
}
