{
  "type": "module",
  "name": "matti-docs",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "files": [
    "dist/",
    "src/",
    "tsconfig.json"
  ],
  "exports": {
    ".": {
      "module": "./dist/components.mjs",
      "types": "./dist/components.d.ts"
    },
    "./package.json": {
      "require": "./package.json",
      "module": "./package.json"
    },
    "./*": {
      "module": "./dist/*.mjs",
      "types": "./dist/*.d.ts"
    }
  },
  "devDependencies": {
    "@types/hast": "^3.0.2",
    "@types/lodash-es": "^4.17.11",
    "@types/node": "^20.8.10",
    "@types/react": "~18.2.25",
    "@types/react-dom": "~18.2.10",
    "@types/react-reconciler": "^0.28.6",
    "@types/uuid": "^9.0.6",
    "@vitejs/plugin-react": "^4.1.1",
    "clean-publish": "^4.2.0",
    "concurrently": "^8.2.2",
    "esbuild": "^0.19.5",
    "eslint": "^8.54.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.28.1",
    "jest-extended": "^4.0.2",
    "prettier": "^3.0.3",
    "type-fest": "^4.6.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@types/react": "18.x",
    "@types/react-dom": "18.x",
    "react": "18.x",
    "react-dom": "18.x",
    "react-reconciler": "^0.29.0"
  },
  "peerDependenciesMeta": {
    "react-reconciler": {
      "optional": true
    },
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "dependencies": {
    "@storybook/react": "^7.5.3",
    "@storybook/react-vite": "^7.5.3",
    "docx": "^8.2.3",
    "es-dirname": "^0.1.0",
    "hast-util-from-html": "^2.0.1",
    "hast-util-from-html-isomorphic": "^2.0.0",
    "hast-util-to-dom": "^4.0.0",
    "hast-util-to-html": "^9.0.0",
    "lodash-es": "^4.17.21",
    "nodemon": "^3.0.1",
    "pagedjs": "^0.4.3",
    "prop-types": "^15.8.1",
    "puppeteer-core": "21.4.1",
    "storybook": "^7.5.3",
    "uuid": "^9.0.1",
    "vitest": "^0.34.6",
    "yalc": "^1.0.0-pre.53"
  },
  "scripts": {
    "clean": "rm -rf dist/*",
    "build": "pnpm clean && pnpm build:all",
    "dev": "pnpm clean && nodemon --exec pnpm run build:all",
    "build:all": "pnpm run \"/^build:(src|types)$/\" && pnpm link:push",
    "build:src": "node esbuild",
    "build:types": "tsc --declaration --emitdeclarationonly --preservewatchoutput",
    "test": "vitest",
    "link:push": "rm -rf ./publish && clean-publish --without-publish --temp-dir publish && yalc push --changed publish && rm -rf ./publish",
    "storybook": "storybook dev"
  },
  "nodemonConfig": {
    "ext": "ts,js,json,css",
    "watch": [
      "src/**",
      "package.json",
      "esbuild.js"
    ],
    "ignore": [
      "**/test/**",
      "**/docs/**"
    ],
    "delay": 1000
  }
}
