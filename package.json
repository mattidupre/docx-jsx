{
  "type": "module",
  "name": "matti-docs",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "files": [
    "dist/",
    "src/"
  ],
  "engines": {
    "node": ">=19",
    "pnpm": ">=8"
  },
  "main": "./dist/components.mjs",
  "exports": {
    "./components": "./dist/components.mjs",
    "./server": "./dist/server.mjs",
    "./headless": "./dist/headless.js",
    "./package.json": "./package.json"
  },
  "devDependencies": {
    "@types/hast": "^3.0.2",
    "@types/react": "~18.2.25",
    "@types/react-dom": "~18.2.10",
    "@types/react-reconciler": "^0.28.6",
    "@types/uuid": "^9.0.6",
    "concurrently": "^8.2.2",
    "esbuild": "^0.19.5",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.28.1",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "dependencies": {
    "docx": "^8.2.3",
    "hast-util-to-html": "^9.0.0",
    "nodemon": "^3.0.1",
    "pagedjs": "^0.4.3",
    "prop-types": "^15.8.1",
    "puppeteer": "^21.4.1",
    "react-reconciler": "^0.29.0",
    "uuid": "^9.0.1",
    "vitest": "^0.34.6",
    "yalc": "^1.0.0-pre.53"
  },
  "scripts": {
    "build": "concurrently \"npm:build:src\" \"npm:build:types\"",
    "build:src": "node esbuild",
    "build:types": "tsc --declaration --emitdeclarationonly",
    "dev": "concurrently \"npm:dev:src\" \"npm:dev:types\" \"npm:dev:link\"",
    "dev:src": "npm run build:src -- --watch",
    "dev:types": "npm run build:types -- --watch",
    "dev:link": "npm exec nodemon -- --preserve-output --watch dist --exec 'npm exec yalc push --changed'",
    "test": "vitest"
  }
}
