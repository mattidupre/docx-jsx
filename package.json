{
  "type": "module",
  "name": "matti-docs",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "files": [
    "dist/",
    "src/",
    "tsconfig.json"
  ],
  "main": "./dist/components.mjs",
  "exports": {
    ".": {
      "default": "./dist/components.mjs",
      "types": "./dist/components.d.ts"
    },
    "./components": {
      "default": "./dist/components.mjs",
      "types": "./dist/components.d.ts"
    },
    "./headless": {
      "require": "./dist/headless.js",
      "types": "./dist/headless.d.ts"
    },
    "./reactToDocx": {
      "default": "./dist/reactToDocx.mjs",
      "types": "./dist/reactToDocx.d.ts"
    },
    "./reactToDom": {
      "default": "./dist/reactToDom.mjs",
      "types": "./dist/reactToDom.d.ts"
    },
    "./reactToPdf": {
      "default": "./dist/reactToPdf.mjs",
      "types": "./dist/reactToPdf.d.ts"
    },
    "./package.json": {
      "default": "./package.json"
    }
  },
  "devDependencies": {
    "@types/hast": "^3.0.2",
    "@types/lodash-es": "^4.17.11",
    "@types/node": "^20.8.10",
    "@types/react": "~18.2.25",
    "@types/react-dom": "~18.2.10",
    "@types/react-reconciler": "^0.28.6",
    "@types/uuid": "^9.0.6",
    "@vitejs/plugin-react": "^4.1.1",
    "clean-publish": "^4.2.0",
    "concurrently": "^8.2.2",
    "esbuild": "^0.19.5",
    "eslint": "^8.54.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.28.1",
    "jest-extended": "^4.0.2",
    "prettier": "^3.0.3",
    "type-fest": "^4.6.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@types/react": "^17.0.0 || ^18.0.0",
    "@types/react-dom": "^17.0.0 || ^18.0.0",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0",
    "react-reconciler": "^0.29.0"
  },
  "peerDependenciesMeta": {
    "react-reconciler": {
      "optional": true
    },
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "dependencies": {
    "docx": "^8.2.3",
    "es-dirname": "^0.1.0",
    "hast-util-to-dom": "^4.0.0",
    "hast-util-to-html": "^9.0.0",
    "lodash-es": "^4.17.21",
    "nodemon": "^3.0.1",
    "pagedjs": "^0.4.3",
    "prop-types": "^15.8.1",
    "puppeteer-core": "21.4.1",
    "uuid": "^9.0.1",
    "vitest": "^0.34.6",
    "yalc": "^1.0.0-pre.53"
  },
  "scripts": {
    "build": "concurrently \"pnpm:build:src\" \"pnpm:build:types\"",
    "build:src": "node esbuild",
    "build:types": "tsc --declaration --emitdeclarationonly --preservewatchoutput",
    "dev": "concurrently \"pnpm:dev:src\" \"pnpm:dev:types\" \"pnpm:dev:link\"",
    "dev:src": "pnpm run build:src --watch",
    "dev:types": "pnpm run build:types --watch",
    "dev:link": "pnpm exec nodemon --delay 3s --watch dist/ --watch package.json --exec 'sleep 5 && pnpm push-link'",
    "test": "vitest",
    "push-link": "rm -rf ./publish && clean-publish --without-publish --temp-dir publish && yalc push --changed publish && rm -rf ./publish"
  }
}
